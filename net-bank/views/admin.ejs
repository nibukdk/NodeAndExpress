<%include ./partials/header.ejs %>
<!-- Form for registration route -->
<div class="col-lg-9 col-sm-6 bodyPart">
      <h1 class="text-dark">Admin Page</h1>
      <p>Welcome Admin </p>
    <ul>
      <% result.forEach((result)=>{ %>

        <li>
          <div class="card border-success bg-dark text-white">
                <h5 class="card-title text-center"><%= result.name %> </h5>
                  <span>Last Logged In: <%= result.last_login_date.toLocaleTimeString().substring(0,8) %>, <%= result.last_login_date.toLocaleDateString("en-GB",{ weekday: 'long', year: 'numeric', month: 'long', day: 'numeric' }) %> </span>
                  <div class="card-footer bg-transparent">

                    <a href="/admin/<%= result._id %>/details" class="card-link">Detail</a>

                  </div>
          </div>

        </li>

  <%  }) %>
    </ul>
</div>

  <%include ./partials/footer.ejs %>
